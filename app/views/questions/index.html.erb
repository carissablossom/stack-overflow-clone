<h1>Questions<h1>

  <% @questions.each do |question| %>
  <p>
    <%= question.title %>
  </p>
  <p>
    <%= question.content %>
  </p>
  <% end %>
  <div>
    <h1>Submit a new Question</h1>
      <%= form_for :question, url: questions_path do |f| %>
      <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
      </p>

      <p>
        <%= f.label :content %><br>
        <%= f.text_area :content %>
      </p>

      <p>
        <%= f.submit %>
      </p>
      <% end %>
  </div>

<% if current_user %>
	<h1>Welcome <%= current_user.username %></h1>
<% end %>
<p><%= @zen_quote %></p>

<h2> All your Questions! </h2>


<div class="questions">
<% @questions.each do |question|%>
  <%= link_to "#{question.title}", question_path(question.id) %>
  <br>
<% end %>
</div>

<br>
<div>
  <%= link_to "Add a Question", new_question_path, {:class=>"question-link"} %>
</div>

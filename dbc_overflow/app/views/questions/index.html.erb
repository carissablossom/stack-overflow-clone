<blockquote>ZEN API QUOTE: <%= @quote %></blockquote>

<textarea class="markdown" name="name"><br> </textarea>

<%= form_for @question do |f| %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= will_paginate %>

<div class='questions'>
  <%= render @questions %>
</div>

<%= will_paginate %>

<h2>DBC Overflow</h2>

<h3>Questions:</h3>
You're in the index!

<div class="questions">
  <ul>
    <% @questions.each do |q| %>
      <li><a href="/questions/<%= q.id %>"><%= q.title %></a></li>
    <% end %>
  </ul>
</div>

<h3>Create a new question</h3>
<%= form_for @question do |f| %>
  <p>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :content %>
    <%= f.text_area :content %>
  </p>
  <%= f.submit %>
<% end %>


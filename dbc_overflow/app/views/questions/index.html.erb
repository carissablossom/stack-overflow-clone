<h3>"<%= @quote.content %>"</h3>


  <p><%= render template: "questions/new.html.erb" %></p>

  <h1>All questions:</h1>
  <p>
  <% @questions.each do |question| %>
    <h3><%= link_to "#{question.title}", question %></h3>
    <p><%= question.content %></p>

    <p>
    <button><%= link_to "Edit", edit_question_path(question) %></button>

    <%= form_for question, :method => :delete do |f| %>
      <%= f.submit  "Delete" %>
    <% end %>

    <%= link_to raw('<i class="fa fa-thumbs-o-up"></i>'), question_path(question)+"/upvotes"%>
    <%= link_to raw('<i class="fa fa-thumbs-o-down"></i>'), question_path(question)+"/downvotes"%>
    <%= question.votes %>
    </p>

  <% end %>
  </p>

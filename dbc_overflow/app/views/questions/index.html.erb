<h1>Listing questions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>
  <% @questions.each do |question| %>
    <tr>
      <td><%= link_to question.title, question_path(question), method: 'get'%></td>
       <td><%= link_to question.content, question_path(question), method: 'get'%></td>
    </tr>
  <% end %>
</table>

<%= render partial: "question" %>

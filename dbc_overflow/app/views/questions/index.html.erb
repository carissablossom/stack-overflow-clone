<div class="container">
  <div id="quote">
    <p><%= @message %></p>
  </div>
  <h1> Question Index</h1>

  <div class="header">
    <h2> Questions: </h2>
  </div>

  <%= render @questions %>

  <div class="question-form">
    <%= form_for @question do |f| %>
      <%= f.label :title %>:
      <%= f.text_field :title %><br />

      <%= f.label :content %>:
      <%= f.text_field :content %><br />

      <%= f.submit %>
    <% end %>
  </div>


</div>

<h1>Welcome to DBC STACK OVERFLOW</h1>
<blockquote>hello</blockquote>
<% @questions.each do |question| %>
  <h1><%= link_to question.title, question %>?</h1>
    <%= button_to "upvote", upvote_question_path(question), method: :post %>
    <%= button_to "downvote", downvote_question_path(question), method: :post %>
  <p><%= question.count %></p>
  <p><%= question.content %></p>
<% end %>

<%= form_for :question do |f| %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>

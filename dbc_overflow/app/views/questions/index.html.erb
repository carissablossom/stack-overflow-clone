<h1>Listing questions</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Content</th>
  </tr>
  <% @questions.each do |question| %>
    <tr>
      <td><%= link_to question.title, questions_path %></td>
      <td><%= link_to question.content, questions_path %></td>
    </tr>
  <% end %>
</table>

<%= render partial: "question" %>

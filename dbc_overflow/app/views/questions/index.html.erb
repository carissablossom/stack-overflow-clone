<div class="container">

  <%= form_for (@question) do |f| %>
  <%= f.label "Title:" %>
  <%= f.text_field :title %><br><br>

  <%= f.label "Content:" %>
  <%= f.text_field :content %><br><br>

  <%= f.submit %>
  <% end %>

  <% @question.errors.full_messages.each do |message| %>
  <br><%= message %><br>
  <% end %>




  <h1>Questions:</h1>
  <div class="question_list">

    <%= render @questions %>
  </div>

</div>

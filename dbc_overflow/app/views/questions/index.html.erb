<h1>QUESTIONS</h1>

<% @questions.each do |question|%>
  <p><%= question.title%>
  <%= question.content%></p>
  <%= link_to "Show Question", controller:"questions", action:"show", id:question %>
<%end%>

<p>
<%= form_for @question do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :content %>
  <%= f.text_field :content %>
</p>

  <%= f.submit %>
<% end %>
</p>



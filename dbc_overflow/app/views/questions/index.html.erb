<h1>QUESTIONS</h1>

<% @questions.each do |question|%>
  <p>
    <span>Title:</span><%= question.title%>
    <span>Content:</span><%= question.content%>
    <span>Votes:</span><%=question.votes%>
    <div><%= link_to image_tag("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTD72DEgGgvvcfmb24bIDNYiQrcY2JkBhjMywx8CoFXMzlQrc7w") %>
      <%= link_to image_tag("https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS1U9PfKvVRtnLSzlGgVgONCNNojxLfHrhBCWHb2Zrk1CZ4xbTEpipvyA") %></div>
  </p>
  <%= link_to "Show Question", controller:"questions", action:"show", id: question.id %>



<%end%>

<p>
<%= form_for @question do |f| %>
<p>
  <%= f.label :title %>
  <%= f.text_field :title %>
</p>

<p>
  <%= f.label :content %>
  <%= f.text_field :content %>
</p>

  <%= f.submit %>
<% end %>




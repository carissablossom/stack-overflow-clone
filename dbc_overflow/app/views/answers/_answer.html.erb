<h4><%= index + 1 %>. <%= answer.title %></h4>
<p><%= answer.content %></p>

<%= link_to raw('<i class="fa fa-thumbs-o-up"></i>'), question_answer_path(@question, answer)+"/upvotes"%>
<%= link_to raw('<i class="fa fa-thumbs-o-down"></i>'), question_answer_path(@question, answer)+"/downvotes"%>

<%= answer.votes %>
